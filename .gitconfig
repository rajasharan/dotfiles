	email = rajasharan@gmail.com
[core]
	editor = /home/linuxbrew/.linuxbrew/bin/vim
	pager = diff-so-fancy | less --tabs=4 -RFX
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = yellow
	frag = magenta bold
	commit = blue
	old = red bold
	new = green
	whitespace = red reverse
